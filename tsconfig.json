{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "moduleResolution": "node16", // or "nodenext" or "bundler"
     "module": "Node16",
    //Added from ChatGPT
    // "module": "esnext" //already needed for React Native
    // "target": "esnext",
    // "jsx": "react-native",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    // "moduleResolution": "node16",
    //From Original Template 
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [  "./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
}
